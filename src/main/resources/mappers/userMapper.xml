<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.yjh.jeansgram.user.dao.UserDAO">  
		
		<insert id="insertUser" parameterType="map">
			INSERT INTO
				`sns_user`
			(
				`loginId`
				, `password`
				, `name`
				, `email`
				, `createdAt`
				, `updatedAt`
			)
			VALUE 
			( 
				#{loginId}
				, #{password}
				, #{name}
				, #{email}
				, now()
				, now()
			)
		</insert>
	</mapper>